{
    "$schema": "https://getcomposer.org/schema.json",
   "name": "admin/api",
    "description": "admin api example",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "catfan/medoo": "^2.2",
        "firebase/php-jwt": "^6.11",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
